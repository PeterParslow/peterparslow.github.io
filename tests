              <font color="orange">codeList</font>='<font color="blue">https://schemas.isotc211.org/schemas/19139/resources/codelist/gmxCodelists.xml#MD_CharacterSetCode</font>' 
              <font color="orange">codeListValue</font>='<font color="blue">8859part1</font>'&gt;<font color="black">ISO/IEC 8859-1 (also known as Latin 1)</font>&lt;/gmd:MD_CharacterSetCode&gt;
         &lt;/gmd:characterSet&gt;
